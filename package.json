    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "gh-pages": "^5.0.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.1",
    "typescript": "5.0.4",
    "vite": "^4.3.9",
    "vite-plugin-node": "^3.0.2",
    "vitest": "^0.32.3"
