  "dependencies": {
    "@koa/cors": "^4.0.0",
    "@koa/router": "^12.0.0",
    "jsonwebtoken": "^9.0.1",
    "koa": "^2.14.2",
    "koa-passport": "^6.0.0",
    "mongoose": "^7.3.2",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "redis": "^4.6.7"
